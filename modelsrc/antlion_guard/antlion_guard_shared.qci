}

$sequence turn_left "UpWalk02" loop X fps 30 frame 30 51 ACT_TURN_LEFT 1 alignto idle {
	ikrule ltftfoot footstep range .. 12 2 ..
	ikrule rtftfoot footstep range .. 1 12 ..
	ikrule ltbkfoot footstep range .. 20 10 ..
