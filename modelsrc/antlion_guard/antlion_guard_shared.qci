}

$sequence run1 "run01" loop LX fps 30 frame 23 36 ACT_ANTLIONGUARD_RUN_FULL 1 origin 0 0 -43.86 alignto idle {
	ikrule ltftfoot footstep range .. 10 1 ..
	ikrule rtftfoot footstep range .. 11 3 ..
